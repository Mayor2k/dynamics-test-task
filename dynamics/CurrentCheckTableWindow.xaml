<Window x:Class="dynamics.CurrentCheckTableWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dynamics"
        mc:Ignorable="d"
        Title="CurrentCheckTableWindow" Height="600" Width="1000">
    <Grid Background="White" ShowGridLines="True" Name="rootGrid">
        <ToolBar VerticalAlignment="Top" Height="25">
            <Button Content="Export" Name="exportButton" Click="exportToExelClick" ClickMode="Press"/>
        </ToolBar>
        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Margin="0, 25, 0, 0">
            <DataGrid IsReadOnly="True" Name="rootDataGrid" AutoGenerateColumns="False">
                <DataGrid.Columns>
                </DataGrid.Columns>
            </DataGrid>
        </ScrollViewer>
    </Grid>
</Window>
